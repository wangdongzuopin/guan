{
  "name": "app-collection-rn",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "renderer": "set BROWSER=none&& expo start --web --max-workers 0 --port 8081",
    "desktop": "concurrently -k \"npm:renderer\" \"wait-on tcp:8081 && electron electron/main.js\"",
    "desktop:pack": "expo export --platform web --output-dir desktop/renderer-dist --max-workers 0 && electron-builder --win",
    "prepare:desktop-cache": "powershell -ExecutionPolicy Bypass -File .\\scripts\\generate-desktop-cache.ps1",
    "type-check": "tsc --noEmit",
    "build:android:apk": "eas build --platform android --profile production-apk",
    "build:android:aab": "eas build --platform android --profile production-aab",
    "build:ios": "eas build --platform ios --profile production-ios",
    "build:harmony": "powershell -ExecutionPolicy Bypass -File .\\harmony\\scripts\\build-hap.ps1"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "expo": "^53.0.0",
    "expo-asset": "~11.1.7",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-speech": "~13.0.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-safe-area-context": "^4.11.1",
    "react-native-send-intent": "^1.3.0",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@types/react": "~18.3.3",
    "concurrently": "^9.2.1",
    "eas-cli": "^14.4.0",
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "expo-build-properties": "~0.14.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.demo.appcollection.desktop",
    "productName": "AppCollectionDesktop",
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "files": [
      "electron/**/*",
      "desktop/renderer-dist/**/*",
      "package.json"
    ],
    "directories": {
      "output": "desktop/dist"
    },
    "win": {
      "target": "nsis"
    }
  }
}
